{
  "name": "language-transliteration",
  "version": "1.0.0",
  "description": "Transliteration JS Library which leverages ULCA APIs for Indic languages",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "scripts": {
    "start": "parcel watch",
    "build": "parcel build",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint . --no-error-on-unmatched-pattern",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "dev": "yarn start & cd example && yarn start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ULCA-IN/language-transliteration.git"
  },
  "types": "dist/types.d.ts",
  "keywords": [
    "Transliterate",
    "React",
    "Indic-languages"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ULCA-IN/language-transliteration/issues"
  },
  "homepage": "https://github.com/ULCA-IN/language-transliteration#readme",
  "dependencies": {
    "@parcel/packager-ts": "^2.8.3",
    "@parcel/transformer-typescript-types": "^2.8.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.6",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/textarea-caret": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-react": "^13.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-standard": "^5.0.0",
    "gh-pages": "^5.0.0",
    "msw": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.8.3",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "textarea-caret": "^3.1.0",
    "typescript": "^4.9.5"
  },
  "files": [
    "dist"
  ]
}
